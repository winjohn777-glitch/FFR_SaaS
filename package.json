  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "@prisma/client": "^5.19.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.4.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "es-abstract": "^1.24.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "framer-motion": "^12.23.18",
    "helmet": "^8.1.0",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.544.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "playwright": "^1.55.1",
    "prisma": "^5.19.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-is": "^19.2.0",
    "react-router-dom": "^7.9.1",
    "react-scripts": "5.0.1",
    "recharts": "^3.2.1",
    "serve": "^14.2.5",
    "sqlite3": "^5.1.7",
    "styled-components": "^6.1.19",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4",
    "zustand": "^5.0.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "setup-db": "sqlite3 database.sqlite < database/schema.sql",
    "seed-data": "sqlite3 database.sqlite < database/seed-data.sql",
    "reset-db": "rm -f database.sqlite && npm run setup-db && npm run seed-data",
    "backend": "nodemon server/server.js",
    "dev": "concurrently \"npm run backend\" \"npm start\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "full-stack": "npm run reset-db && npm run dev",
    "setup": "npm install && npm run reset-db",
    "lint": "eslint src/ server/ --ext .ts,.tsx,.js",
    "lint:fix": "eslint src/ server/ --ext .ts,.tsx,.js --fix",
    "format": "prettier --write src/ server/",
    "type-check": "tsc --noEmit",
    "test:unit": "react-scripts test --coverage --watchAll=false",
    "test:e2e": "cd ../agentic-starter-repo && npx playwright test tests/app-module-inspector.spec.ts",
    "test:all": "npm run test:unit && npm run test:e2e",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "health-check": "curl -f http://localhost:3000 && curl -f http://localhost:5000/health || exit 1",
    "build:production": "NODE_ENV=production npm run build",
    "start:production": "npm run build:production && npx serve -s build -l 80",
    "deploy:production": "./deploy-production.sh",
    "db:migrate:production": "cd backend && NODE_ENV=production npx prisma migrate deploy",
    "db:seed:production": "cd backend && NODE_ENV=production npx prisma db seed",
    "backend:production": "cd backend && NODE_ENV=production npm start",
    "production:full": "npm run build:production && npm run backend:production"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/html2canvas": "^0.5.35",
    "@types/jspdf": "^1.3.3",
    "concurrently": "^9.2.1",
    "eslint": "^8.50.0",
    "jest-environment-jsdom": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.1.0",
    "webpack": "^5.102.1",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-dev-server": "^5.2.2"
  }
}
